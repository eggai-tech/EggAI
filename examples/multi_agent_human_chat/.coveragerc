[run]
source = agents
omit = 
    */tests/*
    */test_*.py
    */*_test_*.py
    */shared_test_utils.py
    */__pycache__/*
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract